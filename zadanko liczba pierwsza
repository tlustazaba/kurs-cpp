
#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Podaj liczbe: ";
    cin >> n;

    if (n < 2) {
        cout << "Nie jest pierwsza";
    }

    int i = 2;
    while (i < n) {
        if (n % i == 0) {
            cout << "Nie jest pierwsza";
        }
        i++;
    }

    cout << "Jest pierwsza";
    return 0;
}
